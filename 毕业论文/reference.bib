

@phdthesis{__2014,
	type = {硕士},
	title = {超声波测距传感器的研究},
	url = {https://kns.cnki.net/kcms2/article/abstract?v=3uoqIhG8C475KOm_zrgu4lQARvep2SAk9z9MrcM-rOU4mSkGl_LWfwJ0vNxg0CGpmz7-UMdu8f_fxzC6fu6utdetacbYsSEk&uniplatform=NZKPT},
	abstract = {在一些特殊场合，如黑暗、低温、电磁干扰等环境，利用操作人员对实时距离测量可能困难，而采用超声波反射法测量距离则能很好的解决该难题。因为超声波反射测量距离是非接触式测量，只要有反射靶位就能对目标距离方便、快捷、精确的测量。本课题设计一套超声波测距系统，该系统以AT89S52单片机为主控芯片，驱动超声波发射接收模块HC-SR04实现测距功能。该系统通过对实时温度进行检测，利用单片机编程对实际声速进行温度补偿，从而消除了因温度引起的声速变化对距离测量影响，使测量结果更加精确。该系统设计了具有人机交互功能界面，采用LCD12864液晶显示界面，同时利用ISD1420语音模块对测量结果进行语音播放。该超声波测距系统的距离测量范围在2cm{\textasciitilde}400cm，最小分辨率为3mm。},
	language = {中文;},
	urldate = {2023-04-22},
	school = {黑龙江大学},
	author = {王, 国军},
	year = {2014},
	keywords = {测距, 超声波, 非接触 Ultrasound, 人机交互, man machine interaction, non-contact, Ranging},
}

